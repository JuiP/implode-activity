msgid "Help"
msgstr "Ayuda"

#: implodeactivity.py:327
msgid "Done"
msgstr "Hecho"

#: implodeactivity.py:374
msgid "Previous"
msgstr "Anterior"

#: implodeactivity.py:375
msgid "Again"
msgstr "Otra vez"

#: implodeactivity.py:376
msgid "Next"
msgstr "Siguiente"

#: implodeactivity.py:404
msgid "Stuck?  You can still solve the puzzle."
msgstr "¿Atascado?  Todavía puedes resolverlo."

#: implodeactivity.py:411
msgid "Undo some moves"
msgstr "Deshacer algunos movimientos"

