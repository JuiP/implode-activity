msgid "Done"
msgstr "Hecho"

#: implodeactivity.py:393
msgid "Previous"
msgstr "Anterior"

#: implodeactivity.py:394
msgid "Again"
msgstr "Otra vez"

#: implodeactivity.py:395
msgid "Next"
msgstr "Siguiente"

#: implodeactivity.py:421
msgid "Stuck?  You can still solve the puzzle."
msgstr "¿Atascado?  Todavía puedes resolverlo."

#: implodeactivity.py:428
msgid "Undo some moves"
msgstr "Deshacer algunos movimientos"
